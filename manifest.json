{
    "name": "Seafile",
    "id": "seafile",
    "description": {
        "en": "Open Source Cloud Storage",
        "fr": "Stockage Cloud Open Source"
    },
    "developer": {
        "name": "ElieSauveterre",
        "email": "contact@mikangali.om",
        "url": "http://www.mikangali.com/"
    },
    "multi_instance": "false",
    "arguments": {
        "install": [
            {
                "name": "domain",
                "ask": {
                    "en": "Choose a domain for Seafile"
                },
                "example": "domain.org",
                "default": "wireynh2.mikangali.com"
            },
            {
                "name": "path",
                "ask": {
                    "en": "Choose a path for Seafile"
                },
                "example": "/seafile",
                "default": "/seafile"
            },
            {
                "name": "server_name",
                "ask": {
                    "en": "Choose a name"
                },
                "example": "Seafile",
                "default": "Seafile"
            }
        ]
    }
}